#' missing_file_ext.R
#'
#' Check if the resource file extension matches the field_form value
#'
#' @param df list: object returned by get_metadata_resources
#' @param credentials list: object returned by the get_credentials() function
#'
#' @return dataframe
#' @export
#'

missing_file_ext <- function(df) {
  complete_formats <- df[!is.na(df$field_formats),]
  # need to map to csv here

  complete_exts <- df[!is.na(df$file_exts),]
}
